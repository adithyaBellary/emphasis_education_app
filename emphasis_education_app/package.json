{
  "name": "emphasis_education_app",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:ios": "react-native bundle --entry-file='index.tsx' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "react-native bundle --entry-file='index.tsx' --bundle-output='./android/main.jsbundle' --dev=false --platform='android' --assets-dest='./android'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.7",
    "@react-native-community/push-notification-ios": "^1.4.1",
    "@react-native-firebase/app": "^8.2.0",
    "@react-native-firebase/messaging": "^7.4.2",
    "@react-navigation/bottom-tabs": "^5.5.1",
    "@react-navigation/native": "^5.1.1",
    "@react-navigation/stack": "^5.2.3",
    "@sentry/react-native": "^1.9.0",
    "@types/crypto-js": "^3.1.44",
    "@types/react-native-push-notification": "^5.0.1",
    "@types/react-navigation": "^3.4.0",
    "@types/styled-components": "^4.4.3",
    "apollo": "^2.28.0",
    "apollo-client": "^2.6.10",
    "apollo-datasource-rest": "^0.8.0",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.20",
    "apollo-server": "^2.11.0",
    "apollo-utilities": "^1.3.3",
    "crypto-js": "^3.1.9-1",
    "firebase": "^7.12.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.3",
    "nodemon": "^2.0.2",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "0.63.3",
    "react-native-elements": "^2.0.0",
    "react-native-fast-image": "^8.3.2",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-gifted-chat": "^0.16.3",
    "react-native-image-picker": "^2.3.2",
    "react-native-paper": "^3.6.0",
    "react-native-push-notification": "^5.1.0",
    "react-native-radio-buttons-group": "^1.0.7",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-vector-icons": "^7.1.0",
    "react-navigation": "^4.3.2",
    "react-navigation-stack": "^2.3.4",
    "react-scripts": "^3.4.1",
    "styled-components": "^5.0.1",
    "subscriptions-transport-ws": "^0.9.16",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.1.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "^0.61.7",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "worker-loader": "^2.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
